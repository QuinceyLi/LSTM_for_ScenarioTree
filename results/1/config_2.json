{
	"data": {
		"filename": "return_data.csv",
		"columns": [
			"stock1",
			"stock2",
			"stock3",
			"stock4",
			"stock5"
		],
		"sequence_length": 50,
		"train_test_split": 0.96,
		"normalise": true,
		"output_idx": [0,1,2,3,4]
	},
	"training": {
		"epochs": 10,
		"batch_size": 32,
		"Num_Sample": 100
	},
	"model": {
		"loss": "mse",
		"optimizer": "adam",
		"save_dir": "saved_models",
		"layers": [
			{
				"type": "lstm",
				"neurons": 100,
				"input_timesteps": 49,
				"input_dim": 5,
				"return_seq": true
			},
			{
				"type": "dropout",
				"rate": 0.2
			},
			{
				"type": "lstm",
				"neurons": 100,
				"return_seq": true
			},
			{
				"type": "lstm",
				"neurons": 100,
				"return_seq": false
			},
			{
				"type": "dropout",
				"rate": 0.2
			},
			{
				"type": "dense",
				"neurons": 10,
				"activation": "linear"
			}
		]
	}
}